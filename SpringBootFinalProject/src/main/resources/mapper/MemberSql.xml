<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sist.data.mapper.MemberMapperInter">
  <insert id="insertMember" parameterType="member">
    insert into member(mem_id,mem_pass,mem_name,mem_email,mem_birth,mem_phone,mem_addr,mem_point) values(#{mem_id},#{mem_pass},#{mem_name},#{mem_email},#{mem_birth},#{mem_phone},#{mem_addr},5000)
  </insert>
  
</mapper>